{
  "version": 2,
  "routes": [
    {
      "src": "/admin",
      "dest": "/admin.php"
    },
    {
      "src": "/contact",
      "dest": "/contact.php"
    },
    {
      "src": "/testimonials",
      "dest": "/testimonials.php"
    },
    {
      "src": "/setup",
      "dest": "/setup.php"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/public/assets/$1"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
